@model IEnumerable<WebApplication1.Entities.Student>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <th scope="row">1</th>
                <td>@item.Firstname</td>
                 <td>@item.Lastname</td>
                 <td>@item.Email</td>
            </tr>
        }

    </tbody>
</table>